			<enumeration value="cultuurhistorie"/>
			<enumeration value="dagrecreatie"/>
			<enumeration value="defensie"/>
			<enumeration value="duurzaamheid"/>
			<enumeration value="ecologie"/>
			<enumeration value="economie"/>
			<enumeration value="energie"/>
			<enumeration value="grondstoffen"/>
			<enumeration value="infrastructuur"/>
			<enumeration value="kabels/leidingen"/>
			<enumeration value="landbouw"/>
			<enumeration value="landschap"/>
			<enumeration value="langzaam verkeer"/>
			<enumeration value="luchtvaart"/>
			<enumeration value="milieu"/>
			<enumeration value="natuur"/>
			<enumeration value="openbaar vervoer"/>
			<enumeration value="recreatie"/>
			<enumeration value="snelverkeer"/>
			<enumeration value="sociaal"/>
			<enumeration value="spoor"/>
			<enumeration value="vaarweg"/>
			<enumeration value="veiligheid"/>
			<enumeration value="verblijfsrecreatie"/>
