<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:imro="http://www.geonovum.nl/imro/2012/1.1" targetNamespace="http://www.geonovum.nl/imro/2012/1.1" elementFormDefault="qualified" version="1.1.1">
	<!--XML Schema document created by ShapeChange-->
	<simpleType name="ThemalijstType">
		<union memberTypes="imro:ThemalijstEnumerationType imro:ThemalijstOtherType"/>
	</simpleType>
	<simpleType name="ThemalijstEnumerationType">
