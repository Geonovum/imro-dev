			<enumeration value="waterkwaliteit"/>
			<enumeration value="waterkwantiteit"/>
			<enumeration value="weg"/>
			<enumeration value="wonen"/>
		</restriction>
	</simpleType>
	<simpleType name="ThemalijstOtherType">
		<restriction base="string">
			<pattern value="other: .*"/>
		</restriction>
	</simpleType>
</schema>
